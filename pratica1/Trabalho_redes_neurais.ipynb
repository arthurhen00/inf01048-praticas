      "source": [
        "# Defina as classes do CIFAR-10\n",
        "\n",
        "#class_names = ['Avião', 'Automóvel', 'Pássaro', 'Gato', 'Cervo', 'Cachorro', 'Sapo', 'Cavalo', 'Navio', 'Caminhão']\n",
